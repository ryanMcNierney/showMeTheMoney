{
  "name": "Show Me The Money",
  "version": "2.1",
  "description": "Highlights sponsored content",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "images/money16.png",
    "48": "images/money48.png",
    "128": "images/money128.png"
  },
  "background": {
    "scripts": [
      "app/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/money16.png",
      "48": "images/money48.png",
      "128": "images/money128.png"
    }
  },
  "manifest_version": 2
}
